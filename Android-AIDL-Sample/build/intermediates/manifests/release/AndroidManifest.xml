<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="net.aminzai.AIDL_Sample.app"
    android:versionCode="1"
    android:versionName="1.0" >

    <permission
        android:name="net.aminzai.AIDL_Sample.service"
        android:description="@string/permission_description"
        android:label="AIDL Sample"
        android:permissionGroup="android.permission-group.NETWORK"
        android:protectionLevel="dangerous" />

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="19" /> <!-- <uses-permission android:name="net.aminzai.AIDL_Sample.service"/> -->
    <application
        android:icon="@drawable/icon"
        android:label="@string/app_name" >
        <activity
            android:name="net.aminzai.AIDL_Sample.AidlSampleActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service
            android:name="net.aminzai.AIDL_Sample.AidlSampleService"
            android:permission="net.aminzai.AIDL_Sample.service"
            android:process=":aidl_service" >
            <intent-filter>
                <action android:name="net.aminzai.AIDL_Sample.IAidlSampleServiceCmd" />
            </intent-filter>
        </service>
    </application>

</manifest>